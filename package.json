{
  "name": "wochenbericht-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "node -e \"try{const fs=require('node:fs');fs.rmSync('.next',{recursive:true,force:true});fs.rmSync('.next-dev',{recursive:true,force:true})}catch(e){}\"",
    "clean:dev": "node -e \"try{require('node:fs').rmSync('.next-dev',{recursive:true,force:true})}catch(e){}\"",
    "clean:build": "node -e \"try{require('node:fs').rmSync('.next',{recursive:true,force:true})}catch(e){}\"",
    "dev": "npm run clean:dev && next dev",
    "build": "npm run clean:build && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "^16.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.17.19",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.6",
    "typescript": "5.7.3"
  }
}
